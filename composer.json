{
    "name": "nc4/comp-cake4-snake-samples",
    "description": "CompCake4SnakeSamples plugin for NC4",
    "type": "nc4-plugin",
    "license": "FreeBSD",
    "require": {
        "php": ">=7.2",
        "nc4/nc4-core": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CompCake4SnakeSamples\\": "src/",
            "CompCake4SnakeSamples\\Test\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
